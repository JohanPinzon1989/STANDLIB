<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Tipo de Iden</th>
      <th scope="col">Num de cedula</th>
      <th scope="col">Nombre</th>
      <th scope="col">Apellido</th>
      <th scope="col">Correo</th>
      <th scope="col">Num fijo</th>
      <th scope="col">Num celular</th>
      <th scope="col">estado/provincia</th>
      <th scope="col">Industria</th>
      <th scope="col">UserName</th>
      <th scope="col" class="text-end">Acciones</th>
    </tr>
  </thead>
  <tbody class="table-group-divider">

    {{#each rows}}
      <tr>
        <th scope="row">{{this.Id}}</th>
        <td>{{this.Tipo_Identificacion_Id}}</td>
        <td>{{this.N_identificacion}}</td>
        <td>{{this.Nombre}}</td>
        <td>{{this.Apellido}}</td>
        <td>{{this.Email}}</td>
        <td>{{this.Num_Fijo}}</td>
        <td>{{this.Num_Celular}}</td>
        <td>{{this.Estado_provincia_Id}}</td>
        <td>{{this.Industria_Id}}</td>
        <td>{{this.username}}</td>
        <td class="text-end">
          <a href="#" type="button" class="btn btn-lidth btn-small"><i
              class="bi bi-eye"
            ></i>
            Ver</a>
          <a href="#" type="button" class="btn btn-lidth btn-small"><i
              class="bi bi-pencil"
            ></i>
            Editar</a>
          <a href="#" type="button" class="btn btn-lidth btn-small"><i
              class="bi bi-person-x"
            ></i>
            Eliminar</a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>